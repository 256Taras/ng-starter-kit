{
  "name": "ng-starter-kit",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "npx eslint 'src/**/*.{js,ts,tsx,html,css,scss}' --quiet",
    "lint:fix": "npx eslint 'src/**/*.{js,ts,tsx,html,css,scss}' --quiet --fix",
    "format": "npx prettier 'src/**/*.{js,ts,tsx,html,css,scss}' --write",
    "dev:ssr": "ng run ng-starter-kit:serve-ssr",
    "serve:ssr": "node dist/ng-starter-kit/server/main.js",
    "build:ssr": "ng build && ng run ng-starter-kit:server",
    "prerender": "ng run ng-starter-kit:prerender",
    "compodoc:build": "compodoc -p tsconfig.doc.json -d ./docs/app ",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s -d ./docs/app",
    "compodoc:serve": "compodoc -s"
  },
  "private": true,
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npm run format",
      "npm run lint:fix"
    ]
  },
  "eslintIgnore": [
    "**/*.scss"
  ],
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.4",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/platform-server": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@nguniversal/express-engine": "^13.1.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "express": "^4.15.2",
    "fp-ts": "^2.12.1",
    "io-ts": "^2.2.16",
    "jest-jasmine2": "^27.5.1",
    "ng-mocks": "^13.4.2",
    "rxjs": "~7.5.0",
    "ts-mockito": "^2.6.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.3",
    "@angular-devkit/build-angular": "~13.3.3",
    "@angular-eslint/eslint-plugin": "^13.2.1",
    "@angular-eslint/eslint-plugin-template": "^13.2.1",
    "@angular-eslint/template-parser": "^13.2.1",
    "@angular/cli": "~13.3.3",
    "@angular/compiler-cli": "~13.3.0",
    "@compodoc/compodoc": "^1.1.19",
    "@nguniversal/builders": "^13.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jasmine": "^4.1.0",
    "jasmine-core": "^4.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.2",
    "lint-staged": "^12.4.0",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "typescript": "~4.6.2"
  }
}
